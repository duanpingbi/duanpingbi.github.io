function mergeArr(arr) {
  if(arr.length &lt; 1) {
      return [];
  }
  let mergedArr = arr[0];
  const merge = function(arrA, arrB) {
      const res = [];
      while(arrA.length &amp;&amp; arrB.length) {
          if(arrA[0] &lt; arrB[0]) {
              res.push(arrA.shift());
          } else {
              res.push(arrB.shift());
          }
      }
      return res.concat(arrA).concat(arrB);
  };</p>

    <p>for(let i = 1;i&lt;arr.length;i++) {
      mergedArr = merge(mergedArr, arr[i]);
  }</p>

    <p>return mergedArr;
}</p>
  </li>
</ul>

